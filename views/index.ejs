  .row-cols-lg-4 {
    display: flex;
    flex-wrap: wrap;
    gap: 3px; /* Small gap for better alignment */
    justify-content: space-between;
  }

  /* Product Card */
  .col {
    margin: 3px; /* Card margin for mobile */
  }

  /* Image Styling */
  .card-img-top {
    object-fit: cover; /* Ensures image looks consistent */
    border-radius: 8px; /* Rounded corners */
    display: block; /* Ensures alignment */
  }

  /* Mobile: Two per row, fixed image size */
  @media (max-width: 576px) {
    .col {
      flex: 1 1 calc(50% - 6px); /* Two per row on mobile with spacing */
      max-width: calc(50% - 6px); /* Proper sizing for mobile */
    }
    .card-img-top {
       /* Fixed width for images */
      width: auto;
      padding: 8px;
      height: 100px; /* Fixed height for images */
       /* No padding on mobile */
    }
  }

  /* Tablet: Three per row */
  @media (min-width: 768px) {
    .col {
      flex: 1 1 calc(33.33% - 20px); /* Three per row */
    }
    .card-img-top {
      width: 100%; /* Full width within container */
      height: auto; /* Maintain aspect ratio */
      padding: 5px; /* Adds padding for better visuals */
    }
  }

  /* Desktop: Four per row, uniform height and spacing */
  @media (min-width: 992px) {
  .col {
    flex: 1 1 calc(25% - 30px); /* Four per row */
  }
  .card-img-top {
    width: 90%; /* Shrinks image within the card */
    height: 160px; /* Slightly shorter */
    padding: 10px; /* Space around image */
    object-fit: contain; /* Prevent stretching */
    margin: 0 auto; /* Center image horizontally */
    border-radius: 10px;
    background-color: #fff; /* Optional: adds contrast behind image */
  }
}

