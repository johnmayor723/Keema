<!-- Styles -->
<style>
  .product-track {
    transition: transform 0.5s ease-in-out;
  }

  .product-slide {
    flex: 0 0 auto;
  }

  /* 8 per screen on large screens (‚â•1200px) */
  @media (min-width: 1200px) {
    .product-slide {
      width: 12.5%; /* 100 / 8 */
    }
  }

  /* 4 per screen on all smaller screens (<1200px) */
  @media (max-width: 1199.98px) {
    .product-slide {
      width: 25%; /* 100 / 4 */
    }
  }
</style>

<!-- JavaScript for Auto Slide -->
<script>
  const track = document.getElementById('productTrack');
  const slides = track.querySelectorAll('.product-slide');
  let index = 0;

  function getSlideWidth() {
    return slides[0].offsetWidth;
  }

  function slideNext() {
    const slideWidth = getSlideWidth();
    const visibleSlides = Math.floor(track.parentElement.offsetWidth / slideWidth);
    const maxIndex = slides.length - visibleSlides;

    index = (index >= maxIndex) ? 0 : index + 1;
    track.style.transform = `translateX(-${index * slideWidth}px)`;
  }

  // Autoplay every 2.5s
  setInterval(slideNext, 2500);

  // Reset transform on window resize
  window.addEventListener('resize', () => {
    const slideWidth = getSlideWidth();
    track.style.transform = `translateX(-${index * slideWidth}px)`;
  });
</script>


